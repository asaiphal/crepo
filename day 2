#include<stdio.h>

int main() {
   int n, i,sum=0;
   float p;
   for(i=1;i<=10;i++)
   {
       printf("number-%d \n",i);
       scanf("%d",&n);
       sum=sum+n;
       
   }
   p=sum/10;
   printf("the sum of 10 natural numbers: %d \n  ",sum);
   printf("the average of 10 numbers is: %f \n ",p);
    
}
