#include<stdio.h>

int main() {
   int a[10], i,sum=0;
   float p;
   for(i=0;i<=9;i++)
   {
              
printf("number-%d \n",i+1);
       scanf("%d",&a[i]);
       sum=sum+a[i];
       
       
   }
   p=sum/10;
   printf("the sum of 10 natural numbers:%d\n  ",sum);
   printf("the avg of 10 natural numbers:%f \n" ,p);
}
