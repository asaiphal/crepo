#include<stdio.h>

int main() {
   int a[5], i,cube=0;
   for(i=0;i<=4;i++)
   {
       
   
       scanf("%d",&a[i]);
       cube=(a[i])*(a[i])*(a[i]);
        printf("Number is : %d and cube of the %d is :%d \n",a[i],a[i],cube);    
   }
  
      }
