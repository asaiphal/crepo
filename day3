Adding 2 3*3 matrices and displaying result in matrix form.

#include <stdio.h>
#include <string.h>
void main()
{
    int a[3][3],b[3][3],i,j,add[3][3];
    for(i=0;i<=2;i++)
    {
    for(j=0;j<=2;j++)
    {
        scanf("%d ",&a[i][j]);
    }
    }
    for(i=0;i<=2;i++)
    {
    for(j=0;j<=2;j++)
    {
        scanf("%d ",&b[i][j]);
    }
    }
    for(i=0;i<=2;i++)
    {
    for(j=0;j<=2;j++)
    {
        add[i][j]=a[i][j]+b[i][j];
    }
    }
    for(i=0;i<=2;i++)
    {
      for(j=0;j<=2;j++)   
    {
        printf("%d  ",add[i][j]);
        if (j==2)
        {
            printf("\n");
        }
    }
    }
    
    
}
